  {
    "name": "2ch Video Highlighter",
    "version": "1.0",
    "description": "Highlight all the webms, mp4s, etc...",
    "icons": {
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.2ch.hk/*"],
        "js": ["utils/index.js", "main.js"]
      }
    ],
    "permissions": [
      "storage", 
      "activeTab"
    ],
    "manifest_version": 2
  }